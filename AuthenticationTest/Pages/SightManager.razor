    @page "/sightmanager"
    @using Radzen
    @using Radzen.Blazor.Rendering
    @using AuthenticationTest.Data.Entities
@using AuthenticationTest.Data
    @using System.Drawing
@using System.IO
@using System.Media
@using AuthenticationTest.Data.Converters.Concrete
@using AuthenticationTest.Data.Seeders
@using Humanizer
@using Microsoft.EntityFrameworkCore.Diagnostics
@using ImageConverter = AuthenticationTest.Data.Converters.Concrete.ImageConverter
@using Path = System.IO.Path
    
@inject NavigationManager NavManager
@inject IJSRuntime JsRuntime
@inject IDAOFetcher _daoFetcher
@inject Company TheCompany
@inject AuthenticationStateProvider auth

    <div class="container">
        <div>
            <h2>Select tour:</h2>
        </div>
    </div>
    <div class="container">
        <div style="margin-right: 5%;">Tour name:</div>
        <div>Sight amount:</div>
        <div style="width: 25%;"></div>
    </div>
    <div class="container">
        @if (Tours.Count == 0 || !Authorized)
        {
            Console.WriteLine("Not authorized or no tours");
            <div>No tours created.</div>
        }
        else
        {
            <div class="scrollpane" style="height: 170px; border-radius: 5px;">
            @foreach (Tour tour in Tours)
            {
                <div>
                    <input readonly @bind="tour.Variants[selectedVariantIndexForToursByIndex[Tours.IndexOf(tour)]].TourName" disabled="@isTheTourOnTheIndexNotSelected[Tours.IndexOf(tour)]"/>
                    <input type="number" readonly value="@tour.Sights.Count" disabled="@isTheTourOnTheIndexNotSelected[Tours.IndexOf(tour)]"/>
                    <select id="languageCodes" @onchange="TourLanguageChanged" name="languageCodes" disabled="@isTheTourOnTheIndexNotSelected[Tours.IndexOf(tour)]">
                        @foreach (string language in tourLanguageCodes[Tours.IndexOf(tour)])
                        {
                            <option>@language</option>
                        }
                    </select>
                    <a href="/TourManager/@tour.Id" class="button-3">Edit Tour</a>
                    <button type="button" class="button-3" @onclick="() => SetTourOnMap(tour.Id, true, true)">Manage sights</button>
                </div>
            }
            </div>
        }
        </div>
    <br>
    <div class="container">
        <a href="/TourManager/0" class="button-3" style="margin: 10px;">Create new tour</a>
        <button @onclick="TogglePlaceNewSightOnClick" class="button-3" hidden="@hideAddMarkerButton">@mapButtonText</button>
    </div>
    <br>
    <div class="container">
        <h3>Editing Tour '@Tours[SelectedTourIndex].Variants[SelectedTourVariantIndex].TourName'</h3>
    </div>
    <div class="container">
        <div>@interactionMessage</div>
        </div>
    <div class="container">
    <div class="row">
        <div style="display: inline-block;">
            <RadzenGoogleMap @ref="MyMap" style="height: 600px; width: 800px;" Zoom="@zoom" ZoomControl="true" Data="@Markers" Center="@(new GoogleMapPosition() { Lat = @lat, Lng = @lng })" MapClick="@onMapClick" MarkerClick="@onMarkerClick"/>
        </div>
        
        <div style="display: inline-block;" hidden="@hideMarkerProperties">
            <div class="scrollpane" style="text-align:center; height: 600px; width: 300px; border-radius: 5px;">
            <div style="margin:auto;">
                <b><label for="languageCodes">Language:</label></b>
                <label for="languageCodes"><i>@selectedTourLanguageCode</i></label>
            </div>

            <div style="margin:auto;">
                <div>
                    <label><b>Sight name: </b></label>
                </div>
                <input placeholder="Sight name..." @bind-value="selectedSight.Variants[selectedSightVariantIndex].SightName">
            </div>

            <div style="margin:auto;">
                <div>
                    <label ><b>Sight description: </b></label>
                </div>
                <input placeholder="Sight description..." @bind-value="selectedSight.Variants[selectedSightVariantIndex].SightDescription">
            </div>

            <div style="margin:auto;">
                <div>
                    <label ><b>Latitude: </b></label>
                </div>
                <input type="number" placeholder="Latitude..." @bind-value="selectedSight.Latitude">
            </div>

            <div style="margin:auto;">
                <div>
                    <label ><b>Longitude: </b></label>
                </div>
                <input type="number" placeholder="Longitude..." @bind-value="selectedSight.Longitude">
            </div>

            <div style="margin:auto;">
                <div>
                    <label ><b>Active radius (meters): </b></label>
                </div>
                <input type="number" placeholder="Radius..." @bind-value="selectedSight.RadiusInMeters">
            </div>

            <div style="margin:auto;">
                <div>
                    <label ><b>Sight image: </b></label>
                </div>
                <img width="300" height="300" src="@ImageDisplay"/>
            </div>

            <div style="margin:auto;">
                <div>
                    <label ><b>Upload image: </b></label>
                </div>
                <InputFile id="image-picker" OnChange="@UploadImage" accept="image/*"/>
            </div>

            <div style="margin:auto;">
                <div>
                    <label ><b>Sight audio: </b></label>
                </div>
                <label >@selectedSight.Variants[selectedSightVariantIndex].AudioFileName</label>
            </div>

            <div style="margin:auto;">
                <div>
                    <label ><b>Upload audio: </b></label>
                </div>
                <InputFile OnChange="@UploadAudio" accept="audio/mpeg"/>
            </div>
            <div style="color:green">@SightEditSuccessMessage</div>
            <div style="color:red">@SightEditErrorMessage</div>
            <button @onclick="UpdateOrSaveSelectedSight" class="button-3">@markerButtonText</button>
        </div>
        </div>
        <div style="display: inline-block;" hidden="@hideSightOverview">
            <div class="container">
                <div><b>Sights in language '@selectedTourLanguageCode':</b></div>
                </div>
            <div class="scrollpane" style="height: 600px; border-radius: 5px;">
                @if (hideSightOverview == false)
                {
                    @foreach (Sight sight in Tours[SelectedTourIndex].Sights)
                    {
                        // We get the sights name in the proper language
                        string theName = "none";
                        foreach (SightVariant variant in sight.Variants)
                        {
                            if (variant.Language.LanguageCode.Equals(selectedTourLanguageCode))
                            {
                                Console.WriteLine(variant.SightName + " is supposedly in " + selectedTourLanguageCode + ", but it is actually in " + variant.Language.LanguageCode);
                                theName = variant.SightName;
                                break;
                            }
                        }
                        <div>
                            <input readonly @bind="@theName"/>
                            <button class="button-3" @onclick="() => SelectMarkerByPosition(sight.Latitude, sight.Longitude)">Choose sight</button>
                        </div>
                    }
                }
            </div>
        </div>
    </div>
    </div>
    @code {
        private int oldAmount;
        private bool Authorized = false;
        // Converters
        private IImageConverter imageConverter = new ImageConverter();
        private IAudioConverter audioConverter = new AudioConverter();
        
        private string ImageDisplay;
        private string AudioToPlay;
        
        private List<string> base64s = new List<string>();

        // Layout
        private string headerMessage = "Select Tour to edit";
        private bool hideMarkerProperties = true;
        private bool hideSightOverview = true;
        private bool placeNewSightOnClick = false;
        private bool hideAddMarkerButton = true;
        private string interactionMessage = "Click on a marker to edit its properties";
        private string mapButtonText = "Place new sight";
        private string markerButtonText = "Update sight";
        private List<bool> isTheTourOnTheIndexNotSelected = new List<bool>();
        private string SightEditSuccessMessage = "";
        private string SightEditErrorMessage = "";
        
        private List<RadzenGoogleMapMarker> Markers = new List<RadzenGoogleMapMarker>();
        
        // Map properties
        private RadzenGoogleMap MyMap = new RadzenGoogleMap();
        private int zoom = 3; // Zoom to start out with
        
        // Map interaction results
        private string clickedPosition = "";
        private double lat = 42.6977;
        private double lng = 23.3219;
        
        // Company Tours and content
        private List<Tour> Tours = new List<Tour>();
        private int SelectedTourIndex = 0;
        private string selectedTourLanguageCode;
        private List<int> selectedVariantIndexForToursByIndex = new List<int>();
        private List<List<string>> tourLanguageCodes = new List<List<string>>();
        private int SelectedSightIndex = 0;
        private int SelectedTourVariantIndex = 0;
        
        private List<string> selectedLanguageCodeForTour = new List<string>();
        
        // Selected marker attributes
        private Sight selectedSight = new Sight();
        private int selectedSightVariantIndex;
        private List<Sight> sightLanguageVariants = new List<Sight>();

        protected override async Task OnInitializedAsync()
        {
            Authorized = IsAuthorized();
            if (Authorized)
            {
                GetToursForCompany();
            }
            
            for (int i = 0; i < Tours.Count; i++)
            {
                selectedVariantIndexForToursByIndex.Add(0);
                List<string> languageCodes = new List<string>();
                for (int j = 0; j < Tours[i].Variants.Count; j++)
                {
                    if (Tours[i].Variants[j].Language != null)
                    {
                        languageCodes.Add(Tours[i].Variants[j].Language.LanguageCode);
                    }
                }
                tourLanguageCodes.Add(languageCodes);
                selectedLanguageCodeForTour.Add(Tours[i].Variants[0].Language.LanguageCode);
            }

            base64s.Add("data:audio/wav;base64,");
        }

        private bool IsAuthorized()
        {
        // We check if the user is logged in
            bool isAuthenticated = auth.GetAuthenticationStateAsync().Result.User.Identity.IsAuthenticated;

            if (!isAuthenticated)
            {
                Tours.Clear();
                // If not authenticated, we redirect to main page
                JsRuntime.InvokeVoidAsync("alert", "NOTICE: This page is only for registered users! Please sign in or register.");
                NavManager.NavigateTo("/");
            }
            return isAuthenticated;
        }
        
        private void GetToursForCompany()
        {
        // Fake tours
        //MockSeedTours();
            
        // Get from DB for user
            Tours = _daoFetcher.TourDao().getToursForCompany(TheCompany.id);

            foreach (Sight sight in Tours[SelectedTourIndex].Sights)
            {
                Console.WriteLine("Variants for " + sight.Id + ": ");
                foreach (var variant in  sight.Variants)
                {
                    Console.WriteLine("-" + variant.SightName + ", Lang: " + variant.Language.LanguageCode);
                }
            }

            Console.WriteLine("Tours found: " + Tours.Count);
            
            for (int j = 0; j < Tours.Count; j++)
            {
                isTheTourOnTheIndexNotSelected.Add(true);
            }
        }
        
        private void TourLanguageChanged(ChangeEventArgs e)
        {
            selectedLanguageCodeForTour[SelectedTourIndex] = (string) e.Value; 
            selectedTourLanguageCode = (string) e.Value;
            for (int i = 0; i < Tours[SelectedTourIndex].Variants.Count; i++)
            {
                if (Tours[SelectedTourIndex].Variants[i].Language.LanguageCode.Equals(selectedTourLanguageCode))
                {
                    SelectedTourVariantIndex = i;
                    selectedVariantIndexForToursByIndex[SelectedTourIndex] = i;
                    break;
                }
            }

            if (Tours[SelectedTourIndex].Sights.Count > 0)
            {
                bool found = false;
                for (int i = 0; i < Tours[SelectedTourIndex].Sights.Count; i++)
                {
                    for (int j = 0; j < Tours[SelectedTourIndex].Sights[i].Variants.Count; j++)
                    {
                        Console.WriteLine("Variant name: " + Tours[SelectedTourIndex].Sights[0].Variants[j].SightName);
                        if (Tours[SelectedTourIndex].Sights[0].Variants[j].Language.LanguageCode.Equals(selectedTourLanguageCode))
                        {
                            selectedSightVariantIndex = j;
                            found = true;
                            break;
                        }
                    }
                    if (found)
                    {
                        break;
                    }
                }
            }
            
            //Console.WriteLine("selectedSightVariantIndex: " + selectedSightVariantIndex + ", name: " + selectedSight.Variants[selectedSightVariantIndex].SightName);

            SetTourOnMap(Tours[SelectedTourIndex].Id, true, false);
            Console.WriteLine("Tour language changed: " + e.Value);
            
            StateHasChanged();
        }

        private void UpdateOrSaveSelectedSight()
        {
            bool updating = selectedSight.Id != 0;
            if (updating) // If we are updating
            {
                UpdateSelectedSight();
            }
            else // If we are creating
            {
                CreateSelectedSight();
            }
        }

        private async void UpdateSelectedSight()
        {
            Console.WriteLine("Updating sight " + selectedSight.Id + "...");
            if (AllFilled())
            {
                UpdateSightOnSelectedTour();
                _daoFetcher.SightDao().CreateVariant(selectedSight, Tours[SelectedTourIndex].Id, selectedTourLanguageCode);
                SetSightUpdatedResultLayout(true);
                await JsRuntime.InvokeVoidAsync("alert", "Sight succesfully updated!");
                Console.WriteLine("New marker mode on: " + placeNewSightOnClick);
                SetTourOnMap(Tours[SelectedTourIndex].Id, true, false);
                StateHasChanged();
            }
        }
        
        private async void CreateSelectedSight()
        {
            // IMPORTANT! Remember to retrieve the new ID after creating
            // And set it for the selected, so that it knows we are updating
            // if the button is pressed again
            Console.WriteLine("Creating sight...");
            if (selectedSight.Variants[0].Language == null)
            {
                selectedSight.Variants[0].Language = new Language
                {
                    LanguageCode = selectedTourLanguageCode
                };
            }
            if (AllFilled())
            {
                AddNewSightToSelectedTour();
                int newId = _daoFetcher.SightDao().CreateNewSightAndVariant(selectedSight, Tours[SelectedTourIndex].Id, selectedTourLanguageCode);
                Console.WriteLine("New ID of new sight: " + newId);
                selectedSight.Id = newId;
                SetSightCreatedResultLayout(true);
                StateHasChanged();
                await JsRuntime.InvokeVoidAsync("alert", "New sight successfully added!");
                await SetTourOnMap(Tours[SelectedTourIndex].Id, true, false);
                Console.WriteLine("New marker mode on: " + placeNewSightOnClick);
                StateHasChanged();
            }
        }

        private void AddNewSightToSelectedTour()
        {
            placeNewSightOnClick = false;
            SetDoNothingOnClickMode();
            
            Tours[SelectedTourIndex].Sights.Add(selectedSight);
        }

        private void UpdateSightOnSelectedTour()
        {
            // We find the sight in questions
            int indexOfSightVariant = 0;
            int indexOfSight = 0;
            for (int i = 0; i < Tours[SelectedTourIndex].Sights.Count; i++)
            {
                if (Tours[SelectedTourIndex].Sights[i].Id == selectedSight.Id)
                {
                    indexOfSight = i;
                    // We find the right variant
                    for (int j = 0; j < Tours[SelectedTourIndex].Sights[i].Variants.Count; j++)
                    {
                        if (Tours[SelectedTourIndex].Sights[i].Variants[j].Language.LanguageCode.Equals(selectedTourLanguageCode))
                        {
                            indexOfSightVariant = j;
                            break;
                        }
                    }
                }
            }
            // We find the variant index in the selected sight
            int selectedSightVariantIndex = 0;
            for (int i = 0; i < selectedSight.Variants.Count; i++)
            {
                if (selectedSight.Variants[i].Language.LanguageCode.Equals(selectedTourLanguageCode))
                {
                    selectedSightVariantIndex = i;
                    break;
                }
            }
            // We set that one to the new values
            Tours[SelectedTourIndex].Sights[indexOfSight].Variants[indexOfSightVariant] = selectedSight.Variants[selectedSightVariantIndex];
        }
        
        private void SetSightUpdatedResultLayout(bool succeeded)
        {
            if (succeeded)
            {
                SightEditSuccessMessage = "Sight succesfully updated!";
                SightEditErrorMessage = "";
            }
            else
            {
                SightEditSuccessMessage = "";
                SightEditErrorMessage = "Failed to update sight!";
            }
        }

        private void SetSightCreatedResultLayout(bool succeeded)
        {
            if (succeeded)
            {
                SightEditSuccessMessage = "New sight succesfully created!";
                SightEditErrorMessage = "";
            }
            else
            {
                SightEditSuccessMessage = "";
                SightEditErrorMessage = "Failed to create new sight!";
            }
        }

        private bool AllFilled()
        {
            Console.WriteLine("Checking if all the neccessary information is filled...");
            bool allFilled = true;

            if (!(selectedSight.Latitude is >= -90 and <= 90))
            {
                Console.WriteLine("Latitude information not filled!");
                SightEditErrorMessage = "Latitude information not filled!";
                return false;
            }
            
            if (!(selectedSight.Longitude is >= -180 and <= 180))
            {
                Console.WriteLine("Longitude information not filled!");
                SightEditErrorMessage = "Longitude information not filled!";
                return false;
            }

            if (selectedSight.ImageBase64 == null)
            {
                Console.WriteLine("ImageBase64 information not filled!");
                SightEditErrorMessage = "ImageBase64 information not filled!";
                return false;
            }

            if (!(selectedSight.RadiusInMeters > 0))
            {
                Console.WriteLine("RadiusInMeters information not filled!");
                SightEditErrorMessage = "RadiusInMeters information not filled!";
                return false;
            }
            
            // We figure out which variant we are working on
            int selectedSightVariantIndex = 0;
            for (int i = 0; i < selectedSight.Variants.Count; i++)
            {
                Console.WriteLine("Variants null: " + (selectedSight.Variants == null));
                Console.WriteLine("Language null: " + (selectedSight.Variants[i].Language == null));
                Console.WriteLine("selectedTourLanguageCode null: " + (selectedTourLanguageCode == null));

                if (selectedSight.Variants[i].Language.LanguageCode.Equals(selectedTourLanguageCode))
                {
                    selectedSightVariantIndex = i;
                    break;
                }
            }
            if (selectedSight.Variants[selectedSightVariantIndex].AudioBase64 == null)
            {
                Console.WriteLine("AudioBase64 information not filled!");
                SightEditErrorMessage = "AudioBase64 information not filled!";
                return false;
            }
            if (selectedSight.Variants[selectedSightVariantIndex].SightDescription == null)
            {
                Console.WriteLine("SightDescription information not filled!");
                SightEditErrorMessage = "SightDescription information not filled!";
                return false;
            }
            if (selectedSight.Variants[selectedSightVariantIndex].SightName == null)
            {
                Console.WriteLine("SightName information not filled!");
                SightEditErrorMessage = "SightName information not filled!";
                return false;
            }
            if (selectedSight.Variants[selectedSightVariantIndex].AudioFileName == null)
            {
                Console.WriteLine("AudioFileName information not filled!");
                SightEditErrorMessage = "AudioFileName information not filled!";
                return false;
            }

            return allFilled;
        }
        
        private void TogglePlaceNewSightOnClick()
        {
            Console.WriteLine("Toggling whether or not to set new sight on click...");
            placeNewSightOnClick = !placeNewSightOnClick;
            if (placeNewSightOnClick)
            {
                SetPlaceNewSightOnClickMode();
            } 
            else
            {
                SetDoNothingOnClickMode();
            }
            Console.WriteLine("Set new marker on click mode: " + placeNewSightOnClick);
            SightEditSuccessMessage = "";
            SightEditErrorMessage = "";
        // We reset the image
            ResetImage();
            RemoveNonTourMarkers(false);
        }

        private void SetPlaceNewSightOnClickMode()
        {
            interactionMessage = "Click on map to create new sight";
            mapButtonText = "Cancel placing new sight";
            hideMarkerProperties = true;
            hideSightOverview = false;
            SightEditSuccessMessage = "";
            SightEditErrorMessage = "";
        }
        
        private void SetDoNothingOnClickMode()
        {
            interactionMessage = "Click on a marker to edit its properties";
            mapButtonText = "Place new sight";           
            hideMarkerProperties = true;
            placeNewSightOnClick = false;
            hideSightOverview = false;
            SightEditSuccessMessage = "";
            SightEditErrorMessage = "";
        }

        private void ResetImage()
        {
            ImageDisplay = imageConverter.Base64ToDisplayableString("");
        }

        private void RemoveNonTourMarkers(bool reset)
        {
            Markers.Clear();
            SetTourOnMap(Tours[SelectedTourIndex].Id, reset, false);
        }

        private void MockSeedTours()
        {
            Tours = TourSeeder.GetSeededToursTList(3, 3);
        }

        private void ClearSelectedSight()
        {
            selectedSight = new Sight();
            selectedSightVariantIndex = 0;
        }

        private async Task SetTourOnMap(int tourId, bool reset, bool initial)
        {
            int newAmount = 0;
            foreach (Tour tour in Tours)
            {
                if (tour.Id == tourId)
                {
                    newAmount = tour.Variants.Count;
                    Console.WriteLine("Old amount: " + oldAmount + ", new amount: " + newAmount);

                    if (oldAmount > newAmount)
                    {
                        tour.Variants.Add(new TourVariant());
                    }
                    oldAmount = tour.Variants.Count;
                }
            }

        // We clear the current tour
            Markers.Clear();
            
            // We clear the current sight
            ClearSelectedSight();
            if (reset)
            {
                SetDoNothingOnClickMode();
            }
            
            // If it is the first time
            if (initial)
            {
                selectedTourLanguageCode = Tours[SelectedTourIndex].Variants[0].Language.LanguageCode;
                selectedSightVariantIndex = 0;
            }
            
            // We show the button
            hideAddMarkerButton = false;

            List<Sight> tourContent = new List<Sight>();
            
            for (int i = 0; i < Tours.Count; i++)
            {
                if (Tours[i].Id == tourId)
                {
                    tourContent = Tours[i].Sights;
                    // We set the tour name in header
                    headerMessage = "Editing Tour '" + Tours[i].Variants[selectedSightVariantIndex].TourName + "'";
                    // And the index
                    SelectedTourIndex = i;
                    // We set the list for bools
                    if (Tours.Count == 1)
                    {
                        isTheTourOnTheIndexNotSelected[0] = false;
                        break;
                    }
                    else
                    {
                        for (int j = 0; j < Tours.Count; j++)
                        {
                            if (SelectedTourIndex == j)
                            {
                                isTheTourOnTheIndexNotSelected[j] = false;
                            }
                            else
                            {
                                isTheTourOnTheIndexNotSelected[j] = true;
                            }
                        }
                        break;
                    }
                }
            }

            for (int i = 0; i < Tours[SelectedTourIndex].Sights.Count; i++)
            {
                int index = 0;
                for (int j = 0; j < Tours[SelectedTourIndex].Sights[i].Variants.Count; j++)
                {
                    if (Tours[SelectedTourIndex].Sights[i].Variants[j].Language.LanguageCode.Equals(selectedTourLanguageCode))
                    {
                        index = j;
                        break;
                    }
                }
                
                GoogleMapPosition newPos = new GoogleMapPosition
                {
                    Lat = Tours[SelectedTourIndex].Sights[i].Latitude,
                    Lng = Tours[SelectedTourIndex].Sights[i].Longitude
                };

                RadzenGoogleMapMarker newMarker = new RadzenGoogleMapMarker()
                {
                    Position = newPos,
                    Label = Tours[SelectedTourIndex].Sights[i].Variants[index].SightName
                };
                
                Console.WriteLine("adding: " + Tours[SelectedTourIndex].Sights[SelectedSightIndex].Variants[index].SightName);
                Markers.Add(newMarker);
            }
            
            selectedTourLanguageCode = selectedLanguageCodeForTour[SelectedTourIndex];
        }

        private void onMapClick(GoogleMapClickEventArgs args)
        {
            Console.WriteLine("Zoom: " + MyMap.Zoom);
            if (!placeNewSightOnClick)
            {
                mapButtonText = "Place new sight";
                hideMarkerProperties = true;
                hideSightOverview = false;
                SightEditSuccessMessage = "";
                SightEditErrorMessage = "";
                return;
            }
            selectedSight = new Sight();
            selectedSight.Variants[0].Language = new Language
            {
                LanguageCode = selectedTourLanguageCode
            };
            selectedSightVariantIndex = 0;
            markerButtonText = "Create new sight";

            RemoveEmptyMarkers();
            
            // We set the map properties
            zoom = 15;
            lat = args.Position.Lat;
            lng = args.Position.Lng;

            RadzenGoogleMapMarker newMarker = new RadzenGoogleMapMarker();
            newMarker.Position = args.Position;
            newMarker.Title = "test";
            newMarker.Label = "New sight marker";
            
            Markers.Add(newMarker);

            selectedSight = new Sight();
            hideMarkerProperties = false;
            hideSightOverview = true;

            SetNewMarkerProperties(newMarker);

            clickedPosition = $"Map clicked LAT : {args.Position.Lat}, LNG : {args.Position.Lng} Zoom: " + MyMap.Zoom;
        }

        private void SetNewMarkerProperties(RadzenGoogleMapMarker newMarker)
        {
            // We set it as the selected marker
            selectedSight = new Sight
            {
                Id = 0,
                Latitude = newMarker.Position.Lat,
                Longitude = newMarker.Position.Lng,
                ImageBase64 = null, // Insert default here
                RadiusInMeters = 10,
                TourId = Tours[SelectedTourIndex].Id
            };
            // And this variant
            SightVariant selectedSightVariant = new SightVariant
            {
                AudioFileName = "No file uploaded",
                AudioBase64 = null, // Insert default here
                Language = new Language
                {
                    LanguageCode = selectedTourLanguageCode
                },
                SightDescription = null,
                SightName = null
            };
            // We add this variant to the Sight
            selectedSight.Variants.Add(selectedSightVariant);
            // We set the selected sight languages
            selectedSightVariantIndex = 0;
        }

        private void RemoveEmptyMarkers()
        {
            for (int i = 0; i < Markers.Count; i++) 
            {
                if (Markers[i].Label.Equals("New sight marker"))
                {
                    Markers.Remove(Markers[i]);
                    break;
                }
            }
        }
        
        private void onMarkerClick(RadzenGoogleMapMarker args)
        {
                if (placeNewSightOnClick)
                {
                  return;
                }

            hideMarkerProperties = false;
            hideSightOverview = true;
            Console.WriteLine("Label: " + args.Label + ", Title: " + args.Title);
            clickedPosition = $"Map {args.Title} clicked LAT : {args.Position.Lat}, LNG : {args.Position.Lng}";
            lat = args.Position.Lat;
            lng = args.Position.Lng;
            
            // We set its info
            SetSelectedMarkerAttributes(args);
        }

        private void SetSelectedMarkerAttributes(RadzenGoogleMapMarker marker)
        {
            Tour selectedTour = Tours[SelectedTourIndex];
            for (int i = 0; i < selectedTour.Sights.Count; i++)
            {
                for (int j = 0; j < selectedTour.Sights.Count; j++)
                {
                    if (selectedTour.Sights[i].Latitude == marker.Position.Lat && selectedTour.Sights[i].Longitude == marker.Position.Lng)
                    {
                        selectedSight = Tours[SelectedTourIndex].Sights[i];
                        if (selectedSight.ImageBase64 != null)
                        {
                            ImageDisplay = imageConverter.Base64ToDisplayableString(selectedSight.ImageBase64);
                        }
                        else
                        {
                            // Set something default
                            ImageDisplay = imageConverter.Base64ToDisplayableString("");
                        }
                        // We set the variant as well
                        selectedSightVariantIndex = -1;
                        for (int k = 0; k < selectedTour.Sights[i].Variants.Count; k++)
                        {
                            if (selectedTour.Sights[i].Variants[k].Language.LanguageCode.Equals(selectedTourLanguageCode))
                            {
                                selectedSightVariantIndex = k;
                                break;
                            }
                        }
                        // Set info for language variants
                        if (selectedSightVariantIndex < 0)
                        {
                            selectedSightVariantIndex = selectedSight.Variants.Count - 1;
                        }
                    }
                }
            }
            
            if (selectedSight.Id == 0)
            {
                markerButtonText = "Create new sight";
            }
            else
            {
                markerButtonText = "Update sight";
            }
        }
        
        private async Task UploadImage(InputFileChangeEventArgs e)
        {
            ImageDisplay = await imageConverter.UploadedFileToDisplayableString(e.File);
            selectedSight.ImageBase64 = await imageConverter.UploadedFileToBase64(e.File);
        }
        
        private async Task UploadAudio(InputFileChangeEventArgs e)
        {
            selectedSight.Variants[selectedSightVariantIndex].AudioBase64 = await audioConverter.UploadedFileToBase64String(e.File);
            selectedSight.Variants[selectedSightVariantIndex].AudioFileName = e.File.Name;
        }

        private void UpdateAudio()
        {
            Console.WriteLine("Updating");
            base64s.Clear();
            base64s.Add("data:audio/wav;base64,");
 
            StateHasChanged();
            Console.WriteLine("Updated");
        }

        private void SelectMarkerByPosition(double lat, double lng)
        {
            foreach (RadzenGoogleMapMarker marker in Markers)
            {
                if (marker.Position.Lat == lat && marker.Position.Lng == lng)
                {
                    lat = marker.Position.Lat;
                    lng = marker.Position.Lng;
                    SetSelectedMarkerAttributes(marker);
                    hideMarkerProperties = false;
                    hideSightOverview = true;
                    break;
                }
            }
        }

